{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@travellers-apps/prices/add-item/feature": [
        "libs/prices/add-item/feature/src/index.ts"
      ],
      "@travellers-apps/prices/auth/feature": [
        "libs/prices/auth/feature/src/index.ts"
      ],
      "@travellers-apps/prices/card/feature": [
        "libs/prices/card/feature/src/index.ts"
      ],
      "@travellers-apps/prices/firestore/feature": [
        "libs/prices/firestore/feature/src/index.ts"
      ],
      "@travellers-apps/prices/home/feature": [
        "libs/prices/home/feature/src/index.ts"
      ],
      "@travellers-apps/prices/page/feature": [
        "libs/prices/page/feature/src/index.ts"
      ],
      "@travellers-apps/prices/shell/feature": [
        "libs/prices/shell/feature/src/index.ts"
      ],
      "@travellers-apps/prices/store/feature": [
        "libs/prices/store/feature/src/index.ts"
      ],
      "@travellers-apps/utils-common": ["libs/utils-common/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
