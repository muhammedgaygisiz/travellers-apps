{
  "name": "common-networkstatus-feature",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/common/networkstatus/feature/src",
  "prefix": "travellers-apps",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/common/networkstatus/feature/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/common/networkstatus/feature/**/*.ts",
          "libs/common/networkstatus/feature/**/*.html"
        ]
      }
    },
    "build": {
      "options": {
        "tsConfig": "libs/common/networkstatus/feature/tsconfig.lib.json"
      }
    }
  },
  "tags": []
}
